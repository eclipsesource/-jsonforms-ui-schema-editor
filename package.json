  "homepage": "https://AhmetTanakol.github.io/jsonforms-ui-schema-editor/",
  "dependencies": {
    "@jsonforms/core": "^2.2.0",
    "@jsonforms/material-renderers": "^2.2.0",
    "@jsonforms/material-tree-renderer": "^2.2.0",
    "@jsonforms/react": "^2.2.0",
    "@material-ui/core": "^3.3.1",
    "@material-ui/icons": "^3.0.1",
    "ajv": "^6.7.0",
    "ajv-errors": "^1.0.1",
    "json-refs": "^3.0.10",
    "material-ui-pickers": "1.0.0",
    "react": "16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^6.0.0",
    "react-scripts": "2.1.3",
    "recompose": "^0.27.1",
    "redux": "^4.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "tslint -p tsconfig.json -c tslint.json 'src/**/*.ts' 'test/*.ts' --exclude src/registerServiceWorker.ts",
    "test": "jest --coverage",
    "coverage": "nyc report --temp-directory=coverage --reporter=text-lcov | coveralls",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js|ts|tsx)$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testURL": "http://localhost/"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/jest": "^23.3.13",
    "@types/lodash": "^4.14.106",
    "@types/node": "10.12.20",
    "@types/react": "16.7.22",
    "@types/react-dom": "16.0.11",
    "coveralls": "^3.0.2",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.4"
  }
